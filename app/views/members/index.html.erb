 <div class="container">
  <div class="row">

    <div class="col-md-3">
      <%= render 'members/member', member: current_member %>

      <h2>New book</h2>
      <%= render 'posts/post', post: @post %>
    </div>
      <div class="col-md-8 offset-md-1">

      <h1>Members</h1>
       <table class="table table-user-show">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
        <% @users.each do |member| %>
          <tr>
             <td><%= image_tag user.get_profile_image(100,100) %></td>
            </td>
              <td><%= member.name %></td>
               <td><%= link_to "Show", member_path(member.id) %></td>



          </tr>
        <% end %>
        </tbody>
      </table>
      </div>
